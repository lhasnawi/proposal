#include "P3DModDB.h"

namespace queueing {




int P3DModDB::getContOutputPortId() const {
    return contOutputPortID;
}

void P3DModDB::setContOutputPortId(int contOutputPortId) {
    contOutputPortID = contOutputPortId;
}

int P3DModDB::getModuleId() const {
    return moduleID;
}

void P3DModDB::setModuleId(int moduleId) {
    moduleID = moduleId;
}

const string& P3DModDB::getModuleName() const {
    return moduleName;
}

void P3DModDB::setModuleName(const string& moduleName) {
    this->moduleName = moduleName;
}

int P3DModDB::getModuleType() const {
    return moduleType;
}

void P3DModDB::setModuleType(int moduleType) {
    this->moduleType = moduleType;
}


} //namespace
